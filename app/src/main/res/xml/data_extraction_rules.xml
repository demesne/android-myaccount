<?xml version="1.0" encoding="utf-8"?>
<data-extraction-rules>
    <cloud-backup>
        <!-- This is for Android 12+ data extraction rules -->
        <include domain="sharedpref" path="."/>
        <exclude domain="sharedpref" path="device.xml"/>
        <exclude domain="sharedpref" path="myaccount_prefs.xml"/>
    </cloud-backup>

    <device-transfer>
        <include domain="sharedpref" path="."/>
        <exclude domain="sharedpref" path="device.xml"/>
        <exclude domain="sharedpref" path="myaccount_prefs.xml"/>
    </device-transfer>
</data-extraction-rules>